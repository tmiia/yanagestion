der:.0625rem solid #dedcda;border-radius:62.5rem}.switch__container label{display:flex}.switch__container label span{flex:1;display:flex;align-items:center;justify-content:center;padding:clamp(.5rem,2vw,1rem)clamp(1rem,4vw,2rem);color:#ff914d;transition:.3s ease-in-out}.switch__container label span:first-of-type{color:#fff}.switch__container label::after{content:"";position:absolute;z-index:-1;top:0;left:0;width:50%;height:100%;background-color:#ff914d;border-radius:62.5rem;transition:.3s ease-in-out}.switch__container .switch{position:absolute;width:100%;height:100%;padding:0;margin:0;opacity:0;cursor:pointer;z-index:3}.switch__container .switch:checked+label span{color:#fff}.switch__container .switch:checked+label span:first-of-type{color:#ff914d}.switch__container .switch:checked+label::after{transform:translateX(100%)}.card{display:flex;border-radius:1rem;padding:clamp(.5rem,2vw,1rem);background-color:rgba(67,89,102,.011);box-shadow:0 0 4px #BFC7CA;overflow:hidden}.card__image{display:flex;justify-content:flex-end}.card__content{margin-bottom:clamp(.25rem,1vw,.5rem)}.card__content .card__title{font-size:clamp(1rem,3vw,1.25rem);margin-bottom:clamp(.25rem,1vw,.5rem);color:#435966}.card__content .card__text{font-size:clamp(.875rem,2vw,1rem);color:#6a7b86;font-weight:300}@media(min-width:500px){.card{flex-direction:row}.card__image{flex:1}.card__image img{transform:translateX(25%)scale(1.3);max-height:190px}.card__content{flex:2;height:fit-content}}@media(max-width:500px){.card{flex-direction:column}.card__image{margin-top:-25%;transform:translate(15%,35%)}}.card.full-img{padding:0;background-color:#435966}.card.full-img .card__image{justify-content:center}.card.full-img .card__image img{transform:translateX(0)scale(1);max-height:unset;opacity:.7}.card.full-img .card__content{display:none}.card.with-img{flex-direction:column-reverse;padding:0}.card.with-img .card__content{flex:1;padding:clamp(.5rem,2vw,1rem)}.card.with-img .card__image{flex:1.5;justify-content:cente